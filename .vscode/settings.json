{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		".eslintcache": true,
		"node_modules": true,
		"dist": true,
		"files": true,
		".idea": true
	},
	"typescript.tsdk": "node_modules\\typescript\\lib",
	"json.schemas": [
		{
			"fileMatch": [
				"/static/api/openapi.json"
			],
			"url": "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/master/schemas/v3.0/schema.json"
		}
	]
}